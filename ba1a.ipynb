      "source": [
        "def PatternCount(pattern,text):\n",
        "  count=0\n",
        "  for i in range(0,len(text)-len(pattern)+1):\n",
        "    if(text[i:i+len(pattern)]==pattern):\n",
        "      count=count+1\n",
        "  return count"
      ]
    },
